def separador() :
     print ("------------")
def agregar_lista  (lista, elemento):
    """
    """
    if elemento in lista:
        try:
            raise ValueError
        except ValueError as err:
            print ('imposible a√±adir elemento duplicado'  + str(elemento))
    else:
        lista.append(elemento)

    print(lista)

        


lista = [0,1,2,3,4]
elemento = 'hola'
agregar_lista(lista,elemento)

separador()
agregar_lista(lista,elemento)
separador()